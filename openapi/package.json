{
  "name": "@paybox/openapi",
  "version": "0.0.6",
  "main": "./src/index.ts",
  "types": "./src/index.d.ts",
  "type": "commonjs",
  "license": "MIT",
  "private": "true",
  "scripts": {
    "build": "esbuild ./src/index.js --bundle --platform=node --outfile=dist/index.js && cp ./src/swagger/swagger.yaml ./dist/swagger.yaml",

    "dev": "node dist/index.js --trace-warning",
    "gen": "openapi-generator generate -i src/swagger/swagger.yaml -g typescript-axios -o ./src/spec-client",
    "genType": "npx openapi-typescript  src/swagger/swagger.yaml -o ./src/openapi.d.ts"
  },
  "dependencies": {
    "@openapitools/openapi-generator-cli": "^2.9.0",
    "@paybox/kafka": "^0.0.6",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.6",
    "global": "^4.4.0",
    "helpful-decorators": "^2.1.0",
    "js-yaml": "^4.1.0",
    "openapi-typescript-codegen": "^0.27.0",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.0",
    "tsoa": "^6.0.1"
  },
  "gitHead": "3753a6aca7d76d07a2999e1a19a271c73d7da1a8",
  "devDependencies": {
    "openapi-typescript": "^7.0.0-next.7"
  }
}

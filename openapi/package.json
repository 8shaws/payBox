    "global": "^4.4.0",
    "helpful-decorators": "^2.1.0",
    "js-yaml": "^4.1.0",
    "openapi-typescript-codegen": "^0.27.0",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.0",
    "tsoa": "^6.0.1"
  },
  "gitHead": "3bd6c7180a38ad8e8b5557751301ffaba6b2a5da"
